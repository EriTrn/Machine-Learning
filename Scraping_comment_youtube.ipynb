    {
      "cell_type": "code",
      "source": [
        "import googleapiclient.discovery\n",
        "import pandas as pd\n",
        "import time\n",
        "\n",
        "api_service_name = \"youtube\"\n",
        "api_version = \"v3\"\n",
        "DEVELOPER_KEY = \""\n",
        "VIDEO_ID = \"SzXMacu80o8\"\n",
        "\n",
        "youtube = googleapiclient.discovery.build(\n",
        "    api_service_name, api_version, developerKey=DEVELOPER_KEY)\n",
        "\n",
        "def get_all_comments(video_id, max_comments=50000):\n",
        "    comments = []\n",
        "    next_page_token = None\n",
        "\n",
        "    while True:\n",
        "        request = youtube.commentThreads().list(\n",
        "            part=\"snippet,replies\",\n",
        "            videoId=video_id,\n",
        "            maxResults=100,\n",
        "            pageToken=next_page_token,\n",
        "            textFormat=\"plainText\"\n",
        "        )\n",
        "        response = request.execute()\n",
        "\n",
        "        for item in response[\"items\"]:\n",
        "            top_comment = item[\"snippet\"][\"topLevelComment\"][\"snippet\"]\n",
        "            comments.append([\n",
        "                top_comment[\"authorDisplayName\"],\n",
        "                top_comment[\"publishedAt\"],\n",
        "                top_comment[\"likeCount\"],\n",
        "                top_comment[\"textDisplay\"],\n",
        "                \"Top-Level\"\n",
        "            ])\n",
        "\n",
        "            # Ambil semua balasan jika ada\n",
        "            reply_count = item[\"snippet\"].get(\"totalReplyCount\", 0)\n",
        "            if reply_count > 0 and \"replies\" in item:\n",
        "                for reply in item[\"replies\"][\"comments\"]:\n",
        "                    reply_snippet = reply[\"snippet\"]\n",
        "                    comments.append([\n",
        "                        reply_snippet[\"authorDisplayName\"],\n",
        "                        reply_snippet[\"publishedAt\"],\n",
        "                        reply_snippet[\"likeCount\"],\n",
        "                        reply_snippet[\"textDisplay\"],\n",
        "                        \"Reply\"\n",
        "                    ])\n",
        "\n",
        "        next_page_token = response.get(\"nextPageToken\")\n",
        "        if not next_page_token or len(comments) >= max_comments:\n",
        "            break\n",
        "\n",
        "        # Hindari rate-limit dengan delay ringan\n",
        "        time.sleep(0.1)\n",
        "\n",
        "    return pd.DataFrame(comments, columns=['author', 'published_at', 'like_count', 'text', 'type'])\n",
        "\n",
        "# Ambil semua komentar + balasan\n",
        "df = get_all_comments(VIDEO_ID, max_comments=9000)"
      ],
